  {
    "name": "Dev Mode",
    "settings": [
      {
        "type": "checkbox",
        "id": "is_dev_mode",
        "label": "Enable dev mode",
        "info": "When working on your local environment, have this enabled in order to point to the correct assets.",
        "default": true
      }
    ]
  },
  {
    "name": "General",
    "settings": [
      {
        "type": "checkbox",
        "id": "use_favicon",
        "label": "Use favicon",
        "default": true
      },
      {
        "type": "image",
        "id": "favicon.png",
        "label": "Favicon",
        "max-width": 16,
        "max-height": 16,
        "info": "16px x 16px .png"
      }
    ]
  },
  {
    "name": "Social media",
