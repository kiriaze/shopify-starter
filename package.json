{
  "name": "shopify-local",
  "version": "1.0.0",
  "description": "Easy Scaffolding powered by StaticLabs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run server | npm run watch",
    "watch": "cd src/theme && theme watch",
    "server": "NODE_ENV=development && webpack-dev-server --inline --hot",
    "build": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "deploy:dev": "npm run build && cd .deploy && theme deploy",
    "deploy:prod": "npm run build && cd .deploy && theme --env production deploy --nodelete",
    "clean": "rm -rf .deploy",
    "update": "npm-check --update"
  },
  "author": "",
  "license": "ISC",
  "browser": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-core": "^6.21.0",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mini-css-extract-plugin": "^0.5.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "0.0.2",
    "fs-extra": "^7.0.1",
    "node-sass": "^4.11.0",
    "npm-check": "^5.9.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": ">=3.1.11"
  },
  "dependencies": {
    "intersection-observer": "^0.5.1",
    "jquery": "^3.3.1",
    "validator": "^10.10.0"
  }
}
