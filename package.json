    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.26.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^0.28.11",
    "dotenv": "^6.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "0.0.1-security",
    "fs-extra": "^2.1.2",
    "node-sass": "^4.9.3",
